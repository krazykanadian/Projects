SUB EraseCursor
IF CursorKind = 10 THEN CIRCLE (CursorX, CursorY), 15, 7
IF CursorKind = 10 THEN CIRCLE (CursorX, CursorY), 14, 7
IF CursorKind = 10 THEN LINE (CursorX - 14, CursorY)-(CursorX + 14, CursorY), 7
IF CursorKind = 10 THEN LINE (CursorX, CursorY - 10)-(CursorX, CursorY + 10), 7
IF CursorKind = 9 THEN CIRCLE (CursorX, CursorY), 18, 7
END SUB

SUB GetInput
MOVE$ = INKEY$
SELECT CASE MOVE$
        CASE "a", "A": IF Turn = 1 THEN PCursorX = PCursorX - 1: EraseCursor
        CASE "d", "D": IF Turn = 1 THEN PCursorX = PCursorX + 1: EraseCursor
        CASE "x", "X": IF Turn = 1 THEN PCursorY = PCursorY + 1: EraseCursor
        CASE "w", "W": IF Turn = 1 THEN PCursorY = PCursorY - 1: EraseCursor
        CASE "s", "S": IF Turn = 1 THEN PlacePiece
        CASE "4": IF Turn = -1 THEN PCursorX = PCursorX - 1: EraseCursor
        CASE "6": IF Turn = -1 THEN PCursorX = PCursorX + 1: EraseCursor
        CASE "2": IF Turn = -1 THEN PCursorY = PCursorY + 1: EraseCursor
        CASE "8": IF Turn = -1 THEN PCursorY = PCursorY - 1: EraseCursor
        CASE "5": IF Turn = -1 THEN PlacePiece
        CASE CHR$(13): END
        END SELECT
IF PCursorX = 9 THEN PCursorX = 1
IF PCursorX = 0 THEN PCursorX = 8
IF PCursorY = 9 THEN PCursorY = 1
IF PCursorY = 0 THEN PCursorY = 8
END SUB

